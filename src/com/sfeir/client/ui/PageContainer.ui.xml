<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	
	<ui:style>
	</ui:style>
	
	<g:AbsolutePanel ui:field="pageContainer">
		<g:SimplePanel ui:field="gridContainer">
			<g:Grid ui:field="grid" cellSpacing="0">
				<g:row>
					<g:customCell>
						<g:Image ui:field="image1" url="image/42.png" />
					</g:customCell>
	
					<g:customCell>
						<g:Image ui:field="image2" url="image/couscous.jpg" />
					</g:customCell>
				</g:row>
				
				<g:row>
					<g:customCell>
						<g:Image ui:field="image3" url="image/justeDebout.jpg" />
					</g:customCell>
	
					<g:customCell>
						<g:Image ui:field="image4" url="image/googleIO.jpg" />
					</g:customCell>
				</g:row>
			</g:Grid>
		</g:SimplePanel>
	</g:AbsolutePanel>
</ui:UiBinder> 